#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2001/XMLSchema#date
xsd:date rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  http://ikm-group.ch/archiMEO/archimate#accesses
archi:accesses rdf:type owl:ObjectProperty ;
               rdfs:domain archi:BehaviourElement ;
               rdfs:range archi:PassiveStructureElement ;
               rdfs:comment "Archimate V2.1 relatioships are determined by nouns: here 'access'" .


###  http://ikm-group.ch/archiMEO/archimate#activeStructureComprisesInterface
archi:activeStructureComprisesInterface rdf:type owl:ObjectProperty ;
                                        rdfs:subPropertyOf emo:aspectCoversObject .


###  http://ikm-group.ch/archiMEO/archimate#activeStructureComprisesStructureElement
archi:activeStructureComprisesStructureElement rdf:type owl:ObjectProperty ;
                                               rdfs:subPropertyOf emo:aspectCoversObject .


###  http://ikm-group.ch/archiMEO/archimate#aggregates
archi:aggregates rdf:type owl:ObjectProperty ;
                 rdfs:domain eo:EnterpriseObject ;
                 rdfs:range eo:EnterpriseObject ;
                 rdfs:comment "The aggregation relationship indicates that a concept groups a number of other concepts (The Open Group)"@en ;
                 rdfs:label "aggregation"@en .


###  http://ikm-group.ch/archiMEO/archimate#assignedTo
archi:assignedTo rdf:type owl:ObjectProperty ;
                 rdfs:domain eo:EnterpriseObject ;
                 rdfs:range eo:EnterpriseObject ;
                 rdfs:comment "Archimate V2.1 relatioships are determined by nouns: here 'assignment'" ;
                 rdfs:label "assignment"@en .


###  http://ikm-group.ch/archiMEO/archimate#associatedWith
archi:associatedWith rdf:type owl:ObjectProperty ;
                     rdfs:domain eo:EnterpriseObject ;
                     rdfs:range eo:EnterpriseObject ;
                     rdfs:comment "An association models a relationship between objects that is not covered  by another, more specific relationship (The Open Group)"@en ;
                     rdfs:label "association"@en .


###  http://ikm-group.ch/archiMEO/archimate#behaviourComprisesBehaviourElement
archi:behaviourComprisesBehaviourElement rdf:type owl:ObjectProperty ;
                                         rdfs:subPropertyOf emo:aspectCoversObject .


###  http://ikm-group.ch/archiMEO/archimate#behaviourComprisesService
archi:behaviourComprisesService rdf:type owl:ObjectProperty ;
                                rdfs:subPropertyOf emo:aspectCoversObject ;
                                rdfs:range archi:Service .


###  http://ikm-group.ch/archiMEO/archimate#composedOf
archi:composedOf rdf:type owl:ObjectProperty ;
                 rdfs:domain eo:EnterpriseObject ;
                 rdfs:range eo:EnterpriseObject ;
                 rdfs:comment "The composition relationship indicates that an object consists of a number  of other objects (The Open Group)"@en ;
                 rdfs:label "composition"@en .


###  http://ikm-group.ch/archiMEO/archimate#flowsTo
archi:flowsTo rdf:type owl:ObjectProperty ;
              rdfs:domain eo:EnterpriseObject ;
              rdfs:range eo:EnterpriseObject ;
              rdfs:comment "The flow relationship describes the exchange or transfer of, for example, information or value between processes, function, interactions, and events (The Open Group)"@en ;
              rdfs:label "flow"@en .


###  http://ikm-group.ch/archiMEO/archimate#groups
archi:groups rdf:type owl:ObjectProperty ;
             rdfs:domain eo:EnterpriseObject ;
             rdfs:range eo:EnterpriseObject .


###  http://ikm-group.ch/archiMEO/archimate#hasAccessTo
archi:hasAccessTo rdf:type owl:ObjectProperty ;
                  rdfs:domain eo:EnterpriseObject ;
                  rdfs:range eo:EnterpriseObject ;
                  rdfs:comment "The access relationship models the access of behavioral concepts to  business or data objects. (The Open Group)"@en ;
                  rdfs:label "access"@en .


###  http://ikm-group.ch/archiMEO/archimate#influence
archi:influence rdf:type owl:ObjectProperty ;
                rdfs:domain eo:EnterpriseObject ;
                rdfs:range eo:EnterpriseObject ;
                rdfs:comment "Archimate V2.1 relatioships are determined by nouns: here 'influence'" ;
                rdfs:label "influence"@en .


###  http://ikm-group.ch/archiMEO/archimate#modelEstablishesMethodsForViewpoint
archi:modelEstablishesMethodsForViewpoint rdf:type owl:ObjectProperty ;
                                          rdfs:domain iso42010:ArchitectureModel ;
                                          rdfs:range iso42010:ArchitectureViewpoint .


###  http://ikm-group.ch/archiMEO/archimate#passiveStructureComprisesObject
archi:passiveStructureComprisesObject rdf:type owl:ObjectProperty ;
                                      rdfs:subPropertyOf emo:aspectCoversObject ;
                                      rdfs:range archi:PassiveStructureElement .


###  http://ikm-group.ch/archiMEO/archimate#realizes
archi:realizes rdf:type owl:ObjectProperty ;
               rdfs:domain eo:EnterpriseObject ;
               rdfs:range eo:EnterpriseObject ;
               rdfs:comment "The realization relationship links a logical entity with a more concrete entity that realizes it (The Open Group)"@en ;
               rdfs:label "realisation"@en .


###  http://ikm-group.ch/archiMEO/archimate#representationRealizesBusinessObject
archi:representationRealizesBusinessObject rdf:type owl:ObjectProperty ;
                                           rdfs:domain archi:Representation ;
                                           rdfs:range archi:BusinessObject .


###  http://ikm-group.ch/archiMEO/archimate#specializes
archi:specializes rdf:type owl:ObjectProperty ;
                  rdfs:domain eo:EnterpriseObject ;
                  rdfs:range eo:EnterpriseObject ;
                  rdfs:comment "Archimate V2.1 relatioships: specialization" .


###  http://ikm-group.ch/archiMEO/archimate#triggers
archi:triggers rdf:type owl:ObjectProperty ;
               rdfs:domain eo:EnterpriseObject ;
               rdfs:range eo:EnterpriseObject ;
               rdfs:comment "The triggering relationship describes the temporal or causal relations between processes, functions, interactions, and events (The Open Group)"@en ;
               rdfs:label "triggering"@en .


###  http://ikm-group.ch/archiMEO/archimate#usedBy
archi:usedBy rdf:type owl:ObjectProperty ;
             rdfs:domain eo:EnterpriseObject ;
             rdfs:range eo:EnterpriseObject ;
             rdfs:comment """The used by relationship models the use of services by processes, functions, or interactions and the access to interfaces by roles, 
 components, or collaborations (The Open Group)"""@en ;
             rdfs:label "used by"@en .


###  http://ikm-group.ch/archiMEO/archimate#viewpointHasSourceLibraryViewpoint
archi:viewpointHasSourceLibraryViewpoint rdf:type owl:ObjectProperty ;
                                         rdfs:domain iso42010:ArchitectureViewpoint ;
                                         rdfs:range archi:LibraryViewpoint .


###  http://ikm-group.ch/archiMEO/emo#aspectCoversObject
emo:aspectCoversObject rdf:type owl:ObjectProperty .


#################################################################
#    Classes
#################################################################

###  http://ikm-group.ch/archiMEO/archimate#Access
archi:Access rdf:type owl:Class ;
             rdfs:subClassOf archi:DependencyRelationships ;
             rdfs:comment """Archimate Relation.
\"The access relationship models the access of behavioral concepts to business or data objects.\" (OpenGroup 2009)""" .


###  http://ikm-group.ch/archiMEO/archimate#Access_Read
archi:Access_Read rdf:type owl:Class ;
                  rdfs:subClassOf archi:Access ;
                  rdfs:comment """Archimate Relation.
\"The access relationship models the access of behavioral concepts to business or data objects.\" (OpenGroup 2009)""" .


###  http://ikm-group.ch/archiMEO/archimate#Access_Write
archi:Access_Write rdf:type owl:Class ;
                   rdfs:subClassOf archi:Access ;
                   rdfs:comment """Archimate Relation.
\"The access relationship models the access of behavioral concepts to business or data objects.\" (OpenGroup 2009)""" .


###  http://ikm-group.ch/archiMEO/archimate#ActiveStructureElement
archi:ActiveStructureElement rdf:type owl:Class ;
                             rdfs:subClassOf eo:EnterpriseObject ;
                             rdfs:comment """Archimate V1.0 Core Concept.
\"The active structure concepts are assigned to behavioral concepts, to show who or what performs the behavior.\" (OpenGroup 2009)""" .


###  http://ikm-group.ch/archiMEO/archimate#Aggregation
archi:Aggregation rdf:type owl:Class ;
                  rdfs:subClassOf archi:StructuralRelationships ;
                  rdfs:comment """Archimate Relation.
\"The aggregation relationship indicates that an object groups a number of other objects.\" (OpenGroup 2009)""" .


###  http://ikm-group.ch/archiMEO/archimate#AndJunction
archi:AndJunction rdf:type owl:Class ;
                  rdfs:subClassOf archi:Junction ;
                  rdfs:comment """subtype of the relationshop connector Junction
\"A junction is used to explicitly express that all elements together must participate in the relationship (and junction)\" (OpenGroup 2009)""" .


###  http://ikm-group.ch/archiMEO/archimate#ApplicationCollaboration
archi:ApplicationCollaboration rdf:type owl:Class ;
                               rdfs:subClassOf archi:ApplicationComponent ;
                               rdfs:comment """Archimate V1.0 Application Layer Concept.
\"An application collaboration defines a (temporary) configuration of two or more components that co-operate to jointly perform application interactions.\" (OpenGroup 2009)""" .


###  http://ikm-group.ch/archiMEO/archimate#ApplicationComponent
archi:ApplicationComponent rdf:type owl:Class ;
                           rdfs:subClassOf archi:ActiveStructureElement ;
                           rdfs:comment """Archimate V1.0 Application Layer Concept.
\"A modular, deployable, and replaceable part of a system that encapsulates its contents and exposes its functionality through a set of interfaces.\" (OpenGroup 2009)""" .


###  http://ikm-group.ch/archiMEO/archimate#ApplicationEvent
archi:ApplicationEvent rdf:type owl:Class ;
                       rdfs:subClassOf archi:BehaviourElement ,
                                       top:Event ;
                       rdfs:comment """Archimate V3.1 Application Layer Concept
\"An application event represents an application state change.\" (OpenGroup 2019).""" .


###  http://ikm-group.ch/archiMEO/archimate#ApplicationFunction
archi:ApplicationFunction rdf:type owl:Class ;
                          rdfs:subClassOf archi:BehaviourElement ;
                          rdfs:comment """Archimate V2.1
\"Application function is defined as behavoir element that groups automated behavoir that can be perfomed by an application component\" (OpenGroup 2013, p.39).""" .


###  http://ikm-group.ch/archiMEO/archimate#ApplicationInteraction
archi:ApplicationInteraction rdf:type owl:Class ;
                             rdfs:subClassOf archi:BehaviourElement ;
                             rdfs:comment """Archimate V2.1
\"An application interaction is defined as the behavoir element that describes the behavoir an application collaboration\" (OpenGroup 2013, p.40).""" .


###  http://ikm-group.ch/archiMEO/archimate#ApplicationInterface
archi:ApplicationInterface rdf:type owl:Class ;
                           rdfs:subClassOf archi:Interface ;
                           rdfs:comment """Archimate V1.0 Application Layer Concept.
\"An application interface declares how a component can connect with its environment.\" (OpenGroup 2009)""" .


###  http://ikm-group.ch/archiMEO/archimate#ApplicationProcess
archi:ApplicationProcess rdf:type owl:Class ;
                         rdfs:subClassOf archi:BehaviourElement ;
                         rdfs:comment """Archimate V3.1 Application Layer Concept
\"An application process represents a sequence of application behaviors that achieves a specific result.\" (OpenGroup 2019).""" .


###  http://ikm-group.ch/archiMEO/archimate#ApplicationService
archi:ApplicationService rdf:type owl:Class ;
                         rdfs:subClassOf archi:Service ;
                         rdfs:comment """Archimate V2.1
\"An application service is defined as a service that exposes automated behavoir\" (OpenGroup 2013, p.41).""" .


###  http://ikm-group.ch/archiMEO/archimate#Artifact
archi:Artifact rdf:type owl:Class ;
               rdfs:subClassOf archi:PassiveStructureElement ;
               rdfs:comment """Archimate V1.0 Technology Layer Concept.
\"A physical piece of information that is used or produced in a software development process, or by deployment and operation of a system.\" (OpenGroup 2009)""" .


###  http://ikm-group.ch/archiMEO/archimate#Assessment
archi:Assessment rdf:type owl:Class ;
                 rdfs:subClassOf archi:MotivationElements ;
                 rdfs:comment """Archimate V3.1 Motivation Layer Concept.
\"An assessment represents the result of an analysis of the state of affairs of the enterprise with respect to some driver.\" (OpenGroup 2019)""" .


###  http://ikm-group.ch/archiMEO/archimate#Assignment
archi:Assignment rdf:type owl:Class ;
                 rdfs:subClassOf archi:StructuralRelationships .


###  http://ikm-group.ch/archiMEO/archimate#Association
archi:Association rdf:type owl:Class ;
                  rdfs:subClassOf archi:DependencyRelationships ;
                  rdfs:comment """Archimate Relation
\"Association models a relationship between objects that is not covered by another, more specific relationship.\" (OpenGroup 2009)""" .


###  http://ikm-group.ch/archiMEO/archimate#BehaviourElement
archi:BehaviourElement rdf:type owl:Class ;
                       rdfs:subClassOf eo:EnterpriseObject ;
                       rdfs:comment "Archimate V1.0 Core Concept" ;
                       rdfs:label "Action" .


###  http://ikm-group.ch/archiMEO/archimate#BusinessActor
archi:BusinessActor rdf:type owl:Class ;
                    rdfs:subClassOf archi:ActiveStructureElement ;
                    rdfs:comment """Archimate V1.0 Business Layer Concept.
\"An organizational entity that is capable of performing behavior.\" (OpenGroup 2009)""" .


###  http://ikm-group.ch/archiMEO/archimate#BusinessBehaviourElement
archi:BusinessBehaviourElement rdf:type owl:Class ;
                               rdfs:subClassOf archi:BehaviourElement ;
                               rdfs:comment """Archimate V1.0 Business Layer Concept.
Internal Business Behaviour""" .


###  http://ikm-group.ch/archiMEO/archimate#BusinessCollaboration
archi:BusinessCollaboration rdf:type owl:Class ;
                            rdfs:subClassOf archi:BusinessRole ;
                            rdfs:comment """Archimate V1.0 Business Layer Concept.
\"A (temporary) configuration of two or more business roles resulting in specific collective behavior in a particular context.\" (OpenGroup 2009)""" .


###  http://ikm-group.ch/archiMEO/archimate#BusinessEvent
archi:BusinessEvent rdf:type owl:Class ;
                    rdfs:subClassOf archi:BusinessBehaviourElement ,
                                    top:Event ;
                    rdfs:comment """Archimate V1.0 Business Layer Concept.
\"Something that happens (internally or externally) and influences behavior.\" (OpenGroup 2009)""" .


###  http://ikm-group.ch/archiMEO/archimate#BusinessFunction
archi:BusinessFunction rdf:type owl:Class ;
                       rdfs:subClassOf archi:BusinessBehaviourElement ;
                       rdfs:comment """Archimate V1.0 Business Layer Concept.
\"A unit of internal behavior that groups behavior according to, for example, required skills, knowledge, resources, etc., and is performed by a single role within the organization.\" (OpenGroup 2009)""" .


###  http://ikm-group.ch/archiMEO/archimate#BusinessInteraction
archi:BusinessInteraction rdf:type owl:Class ;
                          rdfs:subClassOf archi:BusinessBehaviourElement ;
                          rdfs:comment """Archimate V1.0 Business Layer Concept.
\"A unit of behavior performed as a collaboration of two or more business roles.\" (OpenGroup 2009)""" .


###  http://ikm-group.ch/archiMEO/archimate#BusinessInterface
archi:BusinessInterface rdf:type owl:Class ;
                        rdfs:subClassOf archi:Interface ;
                        rdfs:comment """Archimate V1.0 Business Layer Concept.
\"Declares how a business role can connect with its environment.\" (OpenGroup 2009)""" .


###  http://ikm-group.ch/archiMEO/archimate#BusinessObject
archi:BusinessObject rdf:type owl:Class ;
                     rdfs:subClassOf archi:PassiveStructureElement ;
                     rdfs:comment """Archimate V1.0 Business Layer Concept.
\"A unit of information that has relevance from a business perspective.\" (OpenGroup 2009)""" .


###  http://ikm-group.ch/archiMEO/archimate#BusinessProcess
archi:BusinessProcess rdf:type owl:Class ;
                      rdfs:subClassOf archi:BusinessBehaviourElement ;
                      rdfs:comment """Archimate V1.0 Business Layer Concept.
\"A unit of internal behavior or collection of causally related units of internal behavior intended to produce a defined set of products and services.\" (OpenGroup 2009)""" .


###  http://ikm-group.ch/archiMEO/archimate#BusinessRole
archi:BusinessRole rdf:type owl:Class ;
                   rdfs:subClassOf archi:ActiveStructureElement ;
                   rdfs:comment """Archimate V1.0 Business Layer Concept.
\"A named specific behavior of a business actor participating in a particular context.\" (OpenGroup 2009)""" .


###  http://ikm-group.ch/archiMEO/archimate#BusinessService
archi:BusinessService rdf:type owl:Class ;
                      rdfs:subClassOf archi:Service ;
                      rdfs:comment """Archimate V1.0 Business Layer Concept.
\"An externally visible unit of functionality, which is meaningful to the environment and is provided by a business role.\" (OpenGroup 2009)""" ;
                      rdfs:label "" .


###  http://ikm-group.ch/archiMEO/archimate#Capability
archi:Capability rdf:type owl:Class ;
                 rdfs:subClassOf archi:StrategyElements ;
                 rdfs:comment """Archimate V3.1 Strategy Layer Concept.
\"A capability represents an ability that an active structure element, such as an organization, person, or system, possesses.\" (OpenGroup 2019)""" .


###  http://ikm-group.ch/archiMEO/archimate#CommunicationNetwork
archi:CommunicationNetwork rdf:type owl:Class ;
                           rdfs:subClassOf eo:InfrastructureElement ;
                           rdfs:comment """Archimate V3.1 Technology Layer Concept.
\"A communication network represents a set of structures that connects nodes for transmission, routing, and reception of data.\" (OpenGroup 2019)""" .


###  http://ikm-group.ch/archiMEO/archimate#CommunicationPath
archi:CommunicationPath rdf:type owl:Class ;
                        rdfs:subClassOf archi:ActiveStructureElement ;
                        rdfs:comment """Archimate V1.0 Technology Layer Concept.
\"A link between two or more nodes, through which these nodes can exchange information.\" (OpenGroup 2009)""" .


###  http://ikm-group.ch/archiMEO/archimate#CompositeElements
archi:CompositeElements rdf:type owl:Class ;
                        rdfs:subClassOf eo:EnterpriseObject ;
                        rdfs:comment """Archimate V3.1 Core Concept.
\"Composite elements consist of other concepts, possibly from multiple aspects or layers of the language. Grouping and location are generic composite elements (see Figure 18). Composite elements can themselves aggregate or compose other composite elements.\" (OpenGroup 2019)""" ;
                        rdfs:label "CompositeElements" .


###  http://ikm-group.ch/archiMEO/archimate#Composition
archi:Composition rdf:type owl:Class ;
                  rdfs:subClassOf archi:StructuralRelationships .


###  http://ikm-group.ch/archiMEO/archimate#Constraint
archi:Constraint rdf:type owl:Class ;
                 rdfs:subClassOf archi:MotivationElements ;
                 rdfs:comment """Archimate V3.1 Motivation Layer Concept.
\"A constraint represents a factor that limits the realization of goals.\" (OpenGroup 2019)""" .


###  http://ikm-group.ch/archiMEO/archimate#Contract
archi:Contract rdf:type owl:Class ;
               rdfs:subClassOf archi:BusinessObject ;
               rdfs:comment """Archimate V1.0 Business Layer Concept.
\"A formal or informal specification of agreement that specifies the rights and obligations associated with a product.\" (OpenGroup 2009)""" .


###  http://ikm-group.ch/archiMEO/archimate#CourseOfAction
archi:CourseOfAction rdf:type owl:Class ;
                     rdfs:subClassOf archi:StrategyElements ;
                     rdfs:comment """Archimate V3.1 Strategy Layer Concept.
\"A course of action represents an approach or plan for configuring some capabilities and resources of the enterprise, undertaken to achieve a goal.\" (OpenGroup 2019)""" .


###  http://ikm-group.ch/archiMEO/archimate#DataObject
archi:DataObject rdf:type owl:Class ;
                 rdfs:subClassOf archi:PassiveStructureElement ;
                 rdfs:comment """Archimate V2.1
\"A data object is defined as a passive element suitable for automated processing\" (OpenGroup 2013, p.43).""" .


###  http://ikm-group.ch/archiMEO/archimate#Deliverable
archi:Deliverable rdf:type owl:Class ;
                  rdfs:subClassOf archi:ImplementationAndMigrationElements ;
                  rdfs:comment """Archimate V3.1 Implementation Layer Concept.
\"A deliverable represents a precisely-defined result of a work package.\" (OpenGroup 2019)""" .


###  http://ikm-group.ch/archiMEO/archimate#DependencyRelationships
archi:DependencyRelationships rdf:type owl:Class ;
                              rdfs:subClassOf archi:Relationships ;
                              rdfs:comment "Dependency relationships describe how elements support or are used by other elements according to OpenGroup 2019" .


###  http://ikm-group.ch/archiMEO/archimate#Device
archi:Device rdf:type owl:Class ;
             rdfs:subClassOf archi:Node ;
             rdfs:comment """Archimate V1.0 Technology Layer Concept.
\"A physical computational resource upon which artifacts may be deployed for execution.\" (OpenGroup 2009)""" .


###  http://ikm-group.ch/archiMEO/archimate#DistributionNetwork
archi:DistributionNetwork rdf:type owl:Class ;
                          rdfs:subClassOf archi:PhisicalElements ;
                          rdfs:comment """Archimate V3.1 Phisical Layer Concept.
\"A distribution network represents a physical network used to transport materials or energy.\" (OpenGroup 2019)""" .


###  http://ikm-group.ch/archiMEO/archimate#Driver
archi:Driver rdf:type owl:Class ;
             rdfs:subClassOf archi:MotivationElements ;
             rdfs:comment """Archimate V3.1 Motivation Layer Concept.
\"A driver represents an external or internal condition that motivates an organization to define its goals and implement the changes necessary to achieve them.\" (OpenGroup 2019)""" .


###  http://ikm-group.ch/archiMEO/archimate#DynamicRelationships
archi:DynamicRelationships rdf:type owl:Class ;
                           rdfs:subClassOf archi:Relationships ;
                           rdfs:comment "Grouping Construct for Archimate Dynamic Relations according to OpenGroup 2009" .


###  http://ikm-group.ch/archiMEO/archimate#Equipment
archi:Equipment rdf:type owl:Class ;
                rdfs:subClassOf archi:PhisicalElements ;
                rdfs:comment """Archimate V3.1 Phisical Layer Concept.
\"Equipment represents one or more physical machines, tools, or instruments that can create, use, store, move, or transform materials.\" (OpenGroup 2019)""" .


###  http://ikm-group.ch/archiMEO/archimate#Facility
archi:Facility rdf:type owl:Class ;
               rdfs:subClassOf archi:PhisicalElements ;
               rdfs:comment """Archimate V3.1 Phisical Layer Concept.
\"A facility represents a physical structure or environment.\" (OpenGroup 2019)""" .


###  http://ikm-group.ch/archiMEO/archimate#Flow
archi:Flow rdf:type owl:Class ;
           rdfs:subClassOf archi:DynamicRelationships ;
           rdfs:comment """Archimate Relation.
\"The flow relationship describes the exchange or transfer of, for example, information or value between processes, function, interactions, and events.\" (OpenGroup 2009)""" .


###  http://ikm-group.ch/archiMEO/archimate#Gap
